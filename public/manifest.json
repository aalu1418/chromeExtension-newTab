{
  "short_name": "New Tab Weather",
  "name": "New Tab Weather",
  "version": "1.0",
  "description": "New tab extension that shows a clock and weather (with details) - built using React and powered by Dark Sky",

  "manifest_version": 2,
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-N1sEUrl1gfSEI9IlnlAqyv0WExd45knW4yx8b6SAB54='; object-src 'self'",
  "permissions": ["activeTab"]
}
