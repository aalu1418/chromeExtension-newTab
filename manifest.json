{
  "short_name": "New Tab Weather",
  "name": "New Tab Weather",
  "version": "2.0",
  "description": "Simple new tab extension that shows weather",
  "icons": {
    "128": "./favicon.png"
  },
  "manifest_version": 2,
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4='; object-src 'self'",
  "permissions": ["activeTab"]
}
