{
  "short_name": "New Tab Weather",
  "name": "New Tab Weather",
  "version": "1.0",
  "description": "New tab extension that shows a clock, weather, and TTC alerts when in Toronto - built using React and powered by Dark Sky",

  "icons": {"128": "./favicon.png" },

  "manifest_version": 2,
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-w4THnoMYzI/cxW8WBIPLvRof0ou/Mn9fuPSFMHLKuww='; object-src 'self'",
  "permissions": ["activeTab"]
}
